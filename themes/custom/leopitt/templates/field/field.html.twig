{#
/**
 * @file
 * Theme override for a field.
 */
#}
{%
  set field_classes = [
    'field',
    'field-name-' ~ field_name|clean_class,
    'field-type-' ~ field_type|clean_class,
    'field-label-' ~ label_display,
  ]
%}
{% set field_items_attribute = create_attribute() %}
{%
  set field_items_classes = [
    'field-items',
  ]
%}
{%
  set field_item_classes = [
    'field-item',
  ]
%}
{% set title_attribute = create_attribute() %}
{%
  set title_classes = [
    'field-label',
    label_display == 'visually_hidden' ? 'visually-hidden',
  ]
%}
<div{{ attributes.addClass(field_classes) }}>
  {% if not label_hidden %}
    <div{{ title_attributes.addClass(title_classes) }}>{{ label }}</div>
  {% endif %}
  <div{{ field_items_attribute.addClass(field_items_classes) }}>
    {% for item in items %}
      <div{{ item.attributes.addClass(field_item_classes) }}>{{ item.content }}</div>
    {% endfor %}
  </div>
</div>
