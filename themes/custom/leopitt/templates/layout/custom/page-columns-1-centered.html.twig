{#
/**
 * @file
 * 2 column layout.
 */
#}

<div class="page layout-container layout-outer-wrapper columns text-center">
  <div class="column is-6 is-offset-3">
    <div class="page-title">
      {{ content.title }}
    </div>
    <div class="page-preface">
      {{ content.preface }}
    </div>
    <div class="page-main">
      {{ content.main }}
    </div>
    <div class="page-secondary">
      {{ content.secondary }}
    </div>
  </div>
</div>
